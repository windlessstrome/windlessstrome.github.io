{
 "name": "Mitel",
 "main": "index.html",
 "domain": "manhattan",
 "version": "006.3553.104.0",
 "product_string": "Mitel-connect",
 "inDevelopment": true,
 "private": true,
 "node-remote": [
  "https://login.microsoftonline.com/*",
  "http://localhost/*",
  "https://staasf-staging4.shoretelsky.com/*"
 ],
 "scripts": {
  "pack": "./pack-npm.sh",
  "unpack-mac": "./unpack-npm.sh",
  "unpack-win": "unpack-npm",
  "install-dev-server": "npm i -g http-server",
  "start": "npm run configure-dev && npm run start-dev-server",
  "start-dev-server": "http-server -p 4300",
  "configure-dev": "node configure-dev.js"
 },
 "window": {
  "position": "center",
  "width": 430,
  "height": 430,
  "min_width": 430,
  "min_height": 430,
  "show_in_taskbar": true,
  "frame": false,
  "show": false,
  "id": "main",
  "EnableLinkUp": false,
  "icon": "./images/shoretel-icon.png"
 },
 "developer": {
  "loginOnStartup": false,
  "showDevToolsOnStartup": false,
  "throwExceptions": true
 },
 "exo": {
  "minSoftphonePlugin": "0.8.876"
 },
 "repository": {},
 "dependencies": {
  "can": "2.3.33",
  "can-binarytree": "0.0.7",
  "can-derive": "0.0.11",
  "jquery": "1.11.2",
