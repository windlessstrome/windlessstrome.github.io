  "terser": "5.3.2"
 },
 "chromium-args": "--user-data-dir='%APPDATA%'\\Mitel\\'User Data' --disable-features=nw2 --disable-sync --media-cache-size=1 --disable-auth-negotiate-cname-lookup --enable-usermedia-screen-capturing --disable-raf-throttling --enable-precise-memory-info --js-flags='--expose-gc'",
