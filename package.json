  "grunt-shell": "0.6.4",
  "grunt-zip": "0.13.0",
  "bower": "1.3.5",
  "sqlite3": "3.0.4",
  "pbkdf2": "3.0.4",
  "binascii": "0.0.1",
  "node-webkit-updater": "^0.2.3",
  "steal": "0.13.0",
  "polymer": "^1.8.1",
  "webcomponentsjs": "^0.7.0",
  "file-saver": "^1.3.3",
  "localforage": "^1.5.3",
  "shor-avatar": "1.0.5"
 },
 "devDependencies": {
  "grunt-contrib-clean": "0.5.0",
